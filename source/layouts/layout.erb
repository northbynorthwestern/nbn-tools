<!doctype html>
<html>
  <head>
    <meta charset="utf-8">
    
    <!-- Always force latest IE rendering engine or request Chrome Frame -->
    <meta content="IE=edge,chrome=1" http-equiv="X-UA-Compatible">
    
    <!-- Use title if it's in the page YAML frontmatter -->
    <title><%= current_page.data.title || "NBN Web Tools" %></title>
    <link href='http://fonts.googleapis.com/css?family=Lato:100,300,400,700,300italic,400italic' rel='stylesheet' type='text/css'>
    <%= stylesheet_link_tag "normalize", "index" %>
    <%= javascript_include_tag  "vendor/jquery", "script" %>
  </head>
  
  <body class="<%= page_classes %>">
    <%= yield %>
  </body>
</html>